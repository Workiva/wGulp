{
    "languages": ["javascript", "typescript", "coffeescript", "livescript"],
    "path": {
        "root": "./",
        "src": "./src/",
        "test": "./test/",
        "build": "./build/",
        "buildSrc": "./build/src/",
        "buildStyles": "./build/css/",
        "buildTest": "./build/test/",
        "dist": "./dist/",
        "api": "./api/",
        "report": "./report/",
        "coverage": "./report/coverage/",
        "complexity": "./report/complexity/",
        "docs": "./docs/",
        "styles": "./sass/",
        "styleIncludePaths": [],
        "dependencies": ["./jspm_packages"]
    },
    "glob": {
        "all": "**/*",
        "coffee": "**/*.coffee",
        "css": "**/*.css",
        "jsx": "**/*.jsx",
        "ts": "**/*.ts",
        "tsd": "**/*.d.ts",
        "js": "**/*.js",
        "html": "**/*.html",
        "livescript": "**/*.ls",
        "sass": "**/*.sass",
        "scss": "**/*.scss",
        "sources": [ "**/*.css", "**/*.jsx", "**/*.ts", "**/*.js", "**/*.sass", "**/*.scss"],
        "spec": ["**/*Spec.js", "**/*.spec.js", "**/*Spec.ts", "**/*.spec.ts"]
    },
    "compassConfig": "config.rb",
    "jshintrc": "node_modules/wGulp/src/template/.jshintrc",
    "tslintrc": "node_modules/wGulp/src/template/tslint.json",
    "karma": "node_modules/wGulp/karma.conf.js",
    "transforms": [],
    "bundles": {},
    "taskTree": {
        "analyze": ["build"],
        "applyLicense": [],
        "clean": ["clean:buildSrc", "clean:buildStyles", "clean:buildTest", "clean:dist"],
        "clean:buildSrc": [],
        "clean:buildStyles": [],
        "clean:buildTest": [],
        "clean:dist": [],
        "clean:report": [],
        "coffee": ["lint"],
        "compass": ["clean:buildStyles"],
        "concat": ["build"],
        "connect": [],
        "copy": ["clean:buildSrc", "clean:buildStyles", "clean:buildTest"],
        "copy:html": ["clean:buildSrc"],
        "copy:js": ["clean:buildSrc"],
        "copy:jstest": ["clean:buildTest"],
        "cover": ["test"],
        "libraryDist": ["clean", "build"],
        "jasmine": ["preTest"],
        "jsdoc": ["build"],
        "jshint": [],
        "jsx": ["clean:buildSrc", "jshint"],
        "karma": ["preTest"],
        "lint": ["jshint", "tslint"],
        "livescript": ["clean:buildSrc", "lint"],
        "minify": ["minify:js", "minify:css"],
        "minify:css": ["clean:dist", "build"],
        "minify:js": ["clean:dist", "build"],
        "qa": ["default"],
        "sass": ["clean:buildStyles"],
        "serve": [],
        "tsc": ["clean:buildSrc", "tslint", "tsd"],
        "tsc:test": ["clean:buildTest", "tsd"],
        "tsd": [],
        "tslint": [],
        "watch": ["watch:build"],
        "build": ["clean", "lint", "tsd", "jsx", "tsc", "copy:html", "copy:js", "coffee", "livescript", "sass"],
        "bundle": ["clean:dist", "build"],
        "default": ["clean", "build", "test", "analyze", "jsdoc", "dist"],
        "dist": ["clean", "build", "minify", "bundle", "libraryDist"],
        "preTest": ["build", "tsc:test", "copy:jstest"],
        "test": ["preTest", "karma"],
        "test:jasmine": ["preTest", "jasmine"]
    },
    "ts": {
        "sourcemap": true,
        "declaration": false,
        "module": "commonjs",
        "target": "ES5",
        "emitError": true,
        "tmpDir": "/tmp/",
        "noLib": false
    },
    "port": 9000,
    "sauceLabs": {
        "testName": "Unit test run through wGulp",
        "browsers": {
            "sauceChrome": {
                "base": "SauceLabs",
                "browserName": "chrome"
            },
            "sauceFirefox": {
                "base": "SauceLabs",
                "browserName": "firefox"
            },
            "sauceIosSafari": {
                "base": "SauceLabs",
                "browserName": "safari",
                "platform": "OS X 10.9",
                "version": "7"
            },
            "sauceIe9": {
                "base": "SauceLabs",
                "browserName": "internet explorer",
                "platform": "Windows 7",
                "version": "9"
            },
            "sauceIe10": {
                "base": "SauceLabs",
                "browserName": "internet explorer",
                "platform": "Windows 7",
                "version": "10"
            },
            "sauceIe11": {
                "base": "SauceLabs",
                "browserName": "internet explorer",
                "platform": "Windows 7",
                "version": "11"
            }
        }
    }
}

{
    "path": {
        "root": "./",
        "src": "./src/",
        "test": "./test/",
        "build": "./build/",
        "build_src": "./build/src/",
        "build_styles": "./build/css/",
        "build_test": "./build/test/",
        "dist": "./dist/",
        "api": "./api/",
        "functional_test": "./functionalTest/",
        "report": "./report/",
        "coverage": "./report/coverage/",
        "complexity": "./report/complexity/",
        "docs": "./docs/",
        "styles": "./sass/",
        "style_include_paths": [],
        "dependencies": ["./jspm_packages"]
    },
    "glob": {
        "all": "**/*",
        "coffee": "**/*.coffee",
        "css": "**/*.css",
        "jsx": "**/*.jsx",
        "ts": "**/*.ts",
        "tsd": "**/*.d.ts",
        "js": "**/*.js",
        "html": "**/*.html",
        "sass": "**/*.sass",
        "scss": "**/*.scss",
        "sources": ["**/*.css", "**/*.jsx", "**/*.ts", "**/*.js", "**/*.sass", "**/*.scss"],
        "spec": ["**/*Spec.js", "**/*.spec.js", "**/*Spec.ts", "**/*.spec.ts"]
    },
    "compass_config": "config.rb",
    "jshintrc": "node_modules/wGulp/template/.jshintrc",
    "tslintrc": "node_modules/wGulp/template/tslint.json",
    "karma": "node_modules/wGulp/karma.conf.js",
    "nightwatch_config": "node_modules/wGulp/src/nightwatch/nightwatch.json",
    "build_tasks": ["clean", ["lint", "tsd"], ["jsx", "tsc", "copy:html", "copy:js", "sass"]],
    "transforms": [],
    "test_tasks": ["build", ["tsc:test", "copy:jstest"]],
    "dist_tasks": ["clean", "build", "minify", "bundle", "library_dist"],
    "default_tasks": ["test", ["analyze", "jsdoc"], "dist"],
    "bundles": {},
    "ts": {
        "sourcemap": true,
        "declaration": false,
        "module": "commonjs",
        "target": "ES5",
        "emitError": true,
        "tmpDir": "/tmp/",
        "noLib": false
    },
    "port": 9000
}

{
    "languages": ["javascript", "typescript", "coffeescript", "livescript"],
    "path": {
        "root": "./",
        "src": "./src/",
        "test": "./test/",
        "build": "./build/",
        "build_src": "./build/src/",
        "build_styles": "./build/css/",
        "build_test": "./build/test/",
        "dist": "./dist/",
        "api": "./api/",
        "report": "./report/",
        "coverage": "./report/coverage/",
        "complexity": "./report/complexity/",
        "docs": "./docs/",
        "styles": "./sass/",
        "style_include_paths": [],
        "dependencies": ["./jspm_packages"]
    },
    "glob": {
        "all": "**/*",
        "coffee": "**/*.coffee",
        "css": "**/*.css",
        "jsx": "**/*.jsx",
        "ts": "**/*.ts",
        "tsd": "**/*.d.ts",
        "js": "**/*.js",
        "html": "**/*.html",
        "livescript": "**/*.ls",
        "sass": "**/*.sass",
        "scss": "**/*.scss",
        "sources": [ "**/*.css", "**/*.jsx", "**/*.ts", "**/*.js", "**/*.sass", "**/*.scss"],
        "spec": ["**/*Spec.js", "**/*.spec.js", "**/*Spec.ts", "**/*.spec.ts"]
    },
    "compass_config": "config.rb",
    "jshintrc": "node_modules/wGulp/src/template/.jshintrc",
    "tslintrc": "node_modules/wGulp/src/template/tslint.json",
    "karma": "node_modules/wGulp/karma.conf.js",
    "transforms": [],
    "bundles": {},
    "taskTree": {
        "analyze": ["build"],
        "applyLicense": [],
        "clean": [],
        "coffee": ["lint"],
        "compass": ["clean"],
        "concat": ["build"],
        "connect": [],
        "copy": ["clean"],
        "copy:html": ["clean"],
        "copy:js": ["clean"],
        "copy:jstest": ["clean"],
        "cover": ["test"],
        "library_dist": ["clean", "build"],
        "jasmine": ["preTest"],
        "jsdoc": ["build"],
        "jshint": [],
        "jsx": ["clean", "lint"],
        "karma": ["preTest"],
        "lint": ["jshint", "tslint"],
        "livescript": ["clean", "lint"],
        "minify": ["minify:js", "minify:css"],
        "minify:css": ["clean", "build"],
        "minify:js": ["clean", "build"],
        "qa": ["default"],
        "sass": ["clean"],
        "serve": [],
        "tsc": ["clean", "lint", "tsd"],
        "tsc:test": ["clean", "tsd"],
        "tsd": ["clean"],
        "tslint": [],
        "watch": ["watch:build"],
        "build": ["clean", "lint", "tsd", "jsx", "tsc", "copy:html", "copy:js", "coffee", "livescript", "sass"],
        "bundle": ["clean", "build"],
        "default": ["clean", "build", "test", "analyze", "jsdoc", "dist"],
        "dist": ["clean", "build", "minify", "bundle", "library_dist"],
        "preTest": ["build", "tsc:test", "copy:jstest"],
        "test": ["preTest", "karma"],
        "test:jasmine": ["preTest", "jasmine"]
    },
    "ts": {
        "sourcemap": true,
        "declaration": false,
        "module": "commonjs",
        "target": "ES5",
        "emitError": true,
        "tmpDir": "/tmp/",
        "noLib": false
    },
    "port": 9000,
    "sauceLabs": {
        "testName": "Unit test run through wGulp",
        "browsers": {
            "sl_chrome": {
                "base": "SauceLabs",
                "browserName": "chrome"
            },
            "sl_firefox": {
                "base": "SauceLabs",
                "browserName": "firefox"
            },
            "sl_ios_safari": {
                "base": "SauceLabs",
                "browserName": "safari",
                "platform": "OS X 10.9",
                "version": "7"
            },
            "sl_ie_9": {
                "base": "SauceLabs",
                "browserName": "internet explorer",
                "platform": "Windows 7",
                "version": "9"
            },
            "sl_ie_10": {
                "base": "SauceLabs",
                "browserName": "internet explorer",
                "platform": "Windows 7",
                "version": "10"
            },
            "sl_ie_11": {
                "base": "SauceLabs",
                "browserName": "internet explorer",
                "platform": "Windows 7",
                "version": "11"
            }
        }
    }
}
